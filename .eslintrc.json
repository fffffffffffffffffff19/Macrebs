{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": 0,
        "import/no-extraneous-dependencies": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0,
        "no-console": 0,
        "eqeqeq": 0,
        "quotes": 0,
        "quote-props": 0,
        "comma-dangle": 0,
        "no-restricted-syntax": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "new-cap": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "camelcase": 0
    }
}
